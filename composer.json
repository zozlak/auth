{
    "name": "zozlak/auth",
    "type": "library",
    "description": "Very simple yet flexible authentication framework",
    "keywords": ["authentication"],
    "homepage": "https://github.com/zozlak/auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Żółtak",
            "email": "zozlak@zozlak.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "zozlak\\auth\\": "src/zozlak/auth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zozlak\\auth\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^11.4"
    }
}
